 *
 *Problema 03: ESCREVA UM ALGORITMO QUE CALCULE: C = (A + B ) * B.
 */

programa
{
	funcao inicio()
	{real a, b // ALGORITMO A, B
	 
	 
	escreva("Entre o valor de a: ")
	leia(a) // ALGORITMO A
	escreva("Entre o valor de b: ")
	leia(b) // ALGORITMO B

	
	escreva("O valor do Algoritmo C é: ", (a+b)*b) // RESULTADO DO CÁLCULO
	
		
	}
}
